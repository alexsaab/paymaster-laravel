{
    "name": "alexsaab/paymaster-laravel",
    "description": "Paymaster payment method for laravel 5 framework",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Alexey Agafonov",
            "email": "awa77@mail.ru"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": ">=5.4.0",
        "cartalyst/support": "~1.0|~2.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "illuminate/cookie": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/session": "~5.0",
        "ircmaxell/password-compat": "~1.0",
        "mockery/mockery": "~0.9",
        "paragonie/random_compat": "^1.4|^2",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Alexsaab\\Paymaster\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alexsaab\\Paymaster\\PaymasterServiceProvider"
            ]
        }
    }
}
